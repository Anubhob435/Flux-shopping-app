# Flask Configuration
FLASK_SECRET_KEY=your_secret_key_here

# MySQL Database Configuration
MYSQL_HOST=your_mysql_host_here
MYSQL_USER=your_mysql_username_here
MYSQL_PASSWORD=your_mysql_password_here
MYSQL_DATABASE=your_mysql_database_here

# PostgreSQL Database Configuration (optional - for future use)
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require
DATABASE_URL_UNPOOLED=postgresql://username:password@host:port/database?sslmode=require

# PostgreSQL Connection Parameters (optional - for future use)
PGHOST=your_postgres_host_here
PGHOST_UNPOOLED=your_postgres_host_here
PGUSER=your_postgres_username_here
PGDATABASE=your_postgres_database_here
PGPASSWORD=your_postgres_password_here
PGPORT=5432

# Vercel Postgres Templates Variables (optional - for future use)
POSTGRES_URL=postgresql://username:password@host:port/database?sslmode=require
POSTGRES_URL_NON_POOLING=postgresql://username:password@host:port/database?sslmode=require
POSTGRES_USER=your_postgres_username_here
POSTGRES_HOST=your_postgres_host_here
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DATABASE=your_postgres_database_here
POSTGRES_URL_NO_SSL=postgresql://username:password@host:port/database
POSTGRES_PRISMA_URL=postgresql://username:password@host:port/database?connect_timeout=15&sslmode=require

# Neon Auth Environment Variables for Next.js (optional - for future use)
NEXT_PUBLIC_STACK_PROJECT_ID=your_project_id_here
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=your_publishable_key_here
STACK_SECRET_SERVER_KEY=your_secret_server_key_here